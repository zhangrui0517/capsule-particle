{
  // 项目名称
  "name": "map-tree",
  // 版本号
  "version": "1.0.0",
  // 项目描述
  "description": "一个用于操作Map树数据结构的JavaScript库",
  // CommonJS格式入口
  "main": "dist/map-tree.js",
  // ES模块格式入口
  "module": "dist/map-tree.mjs",
  // 浏览器环境UMD格式入口
  "browser": "dist/map-tree.umd.js",
  // TypeScript类型声明文件
  "types": "dist/index.d.ts",
  // 项目脚本命令
  "scripts": {
    // 开发模式启动Vite服务器
    "dev": "vite",
    // 构建项目
    "build": "vite build",
    // 预览构建结果
    "preview": "vite preview",
    // 发布前运行构建
    "prepublishOnly": "npm run build"
  },
  // 项目关键词
  "keywords": [
    "map",
    "tree",
    "data-structure"
  ],
  // 作者
  "author": "",
  // 许可证
  "license": "MIT",
  // 开发依赖
  "devDependencies": {
    // TypeScript
    "typescript": "^4.9.5",
    // Vite构建工具
    "vite": "^5.0.0",
    // Vite插件：生成TypeScript声明文件
    "vite-plugin-dts": "^3.6.0"
  },
  // 发布到npm时包含的文件
  "files": [
    "dist"
  ]
} 